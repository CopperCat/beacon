<h1>Listing booths</h1>

<table>
  <thead>
    <tr>
      <th>X pos</th>
      <th>Y pos</th>
      <th>Width</th>
      <th>Height</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booths.each do |booth| %>
      <tr>
        <td><%= booth.x_pos %></td>
        <td><%= booth.y_pos %></td>
        <td><%= booth.width %></td>
        <td><%= booth.height %></td>
        <td><%= link_to 'Show', booth %></td>
        <td><%= link_to 'Edit', edit_booth_path(booth) %></td>
        <td><%= link_to 'Destroy', booth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booth', new_booth_path %>
